{
    "name": "daydreamlab/user",
    "description": "DaydreamLab User",
    "authors": [
        {
            "name": "Jordan Tsai",
            "email": "jordan@daydream-lab.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/database/seeds"
        ],
        "psr-4": {
            "DaydreamLab\\User\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "daydreamlab/jjaj": "dev-master",
        "kalnoy/nestedset": "^v5.0",
        "laravel/socialite": "^v4.0",
        "laravel/passport": "^7.0"
    },
    "repositories" : [
        {
            "type": "vcs",
            "url":  "https://github.com/DaydreamLab/user",
            "no-api": true
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DaydreamLab\\User\\UserServiceProvider"
            ]
        }
    }
}
